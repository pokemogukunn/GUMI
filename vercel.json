{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "blog/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/blog/p/qvzimjn/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    }
  ]
}
